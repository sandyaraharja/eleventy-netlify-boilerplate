---
layout: layouts/base.njk
section: contact
---

	<!-- Foto pembatas -->
	<section class="page-top-section set-bg" data-setbg="{{ '/' | url }}_includes/assets/img/page-top-bg/4.jpg">
	<div class="page-info">
		<h2>{{ title }}</h2>
		<div class="site-breadcrumb">
			<a href="">{{ title }}</a>  /
			<span>{{ title }}</span>
		</div>
	</div>
	</section>
	<!-- foto pembatas -->


	<!-- Contact page -->
	<section class="contact-page">
		<div class="container">
			{{ layoutContent | safe }}
		</div>
	</section>
	<!-- Contact page end-->


	<!-- Newsletter section -->
	<section class="newsletter-section">
		<div class="container">
			<h2>Subscribe to our newsletter</h2>
			<form class="newsletter-form">
				<input type="text" placeholder="ENTER YOUR E-MAIL">
				<button class="site-btn">subscribe <img src="{{ '/' | url }}_includes/assets/img/icons/double-arrow.png" alt="#"/></button>
			</form>
		</div>
	</section>
	<!-- Newsletter section end -->

